{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/**/*",
    "src/renderer/**/*.vue",
    "src/preload/index.d.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/renderer/src/*"]
    },
    "types": ["vite/client"]
  }
}